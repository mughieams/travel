<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for travel.order -->
        <record id="seq_travel_order" model="ir.sequence">
            <field name="name">Travel Order</field>
            <field name="code">travel.order</field>
            <field name="prefix">TO</field>
            <field name="padding">3</field>
        </record>
		
		<!-- Sequences for travel.schedule -->
        <record id="seq_travel_schedule" model="ir.sequence">
            <field name="name">Travel Schedule</field>
            <field name="code">travel.schedule</field>
            <field name="prefix">SCH</field>
            <field name="padding">3</field>
        </record>

    </data>
</odoo>