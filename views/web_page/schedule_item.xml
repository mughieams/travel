<?xml version="1.0"?>
<odoo>
	<data>
		
		<!-- Halaman List Pool -->
		<template id="schedule-item">
			<t t-call="website.layout">
                <t t-set="title">Travel</t>
				    <div class="oe_structure">
					<div class="container">
						<br/>
						<div class="jumbotron panel panel-default o_panel s_panel">
							<div class="panel-heading">
								<h3 class="panel-title o_default_snippet_text">Travel Schedule</h3>
							</div>
							<div class="panel-body">

							<table class="table table-bordered">
								<thead>
									<tr>
										<th class="col-md-5">Departure</th>
										<th class="col-md-5">Destination</th>
										<th class="col-md-4">Departure Date</th>
										<th class="col-md-3">Departure Time</th>
									</tr>
								</thead>
								<tbody>
									<tr>
										<td><t t-esc="schedule.departure"/></td>
										<td><t t-esc="schedule.destination"/></td>
										<td><t t-esc="schedule.departure_date"/></td>
										<td><t t-esc="schedule.departure_time"/></td>
									</tr>
								</tbody>
							</table>
							
							</div>
							
							<button class="btn btn-default">
								Order
							</button>
						
						</div>
                    </div>
                </div>
            </t>
		</template>
			
	</data>
</odoo>